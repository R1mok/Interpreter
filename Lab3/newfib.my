value fib(value n){
    zero? (n){
        return 0;
    }
    zero? (n-1){
        return 1;
    }
    value n1 = n - 1;
    value n2 = n - 2;
    value newN1 = fib(n1);
    value newN2 = fib(n2);
    value ret = newN1 + newN2;
    return ret;
}
